{
  "name": "content_viewer",
  "version": "0.0.1",
  "description": "Reddit viewer based on React + Flux + Webpack",
  "main": "index.js",
  "scripts": {
    "mock-server": "node server/server_mock.js",
    "test:single": "rm -rf tmp/ && karma start karma.conf.js --single-run",
    "test": "karma start",
    "precommit": "npm run lint",
    "lint": "eslint --cache --ignore-path .gitignore --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --content-base app",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "validate": "npm ls"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "express",
    "sass",
    "flux"
  ],
  "author": "Ruslan Malogulko",
  "license": "ISC",
  "dependencies": {
    "babel-istanbul": "^0.12.2",
    "babel-istanbul-loader": "^0.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "enzyme": "^2.8.2",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "history": "^4.6.1",
    "jasmine-core": "^2.6.2",
    "karma-chrome-launcher": "^2.1.1",
    "phantomjs-polyfill-object-assign": "^0.0.2",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "2.8.1",
    "sass-loader": "^6.0.5",
    "shallow": "^1.0.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.0",
    "husky": "^0.13.3",
    "jquery": "^3.2.1",
    "karma": "^1.7.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.2",
    "open-browser-webpack-plugin": "^0.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-middleware": "^0.1.21",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.17.0",
    "validate-commit-msg": "^2.12.1"
  }
}
